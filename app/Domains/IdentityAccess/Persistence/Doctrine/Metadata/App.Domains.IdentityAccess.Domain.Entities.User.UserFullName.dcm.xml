<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <embeddable name="App\Domains\IdentityAccess\Domain\Entities\User\UserFullName" table="users">
        <field name="firstName" type="string" column="user_first_name" length="64" nullable="false"/>
        <field name="lastName" type="string" column="user_last_name" length="64" nullable="false"/>
    </embeddable>
</doctrine-mapping>